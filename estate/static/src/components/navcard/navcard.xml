<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="estate.Navcard" owl="1">
        <div class="navcard" >
            <div class="card-body">
                <t t-if="props.slots.title">
                    <h3 class="card-title"> <t t-slot="title"/></h3>     
                </t>

                <h2 class="card-text">
                    <t t-slot="default"/>
                </h2>

            </div>
        </div>
    </t>
</templates>

